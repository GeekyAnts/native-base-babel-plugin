{
  "name": "@native-base/babel-plugin",
  "version": "1.0.4",
  "description": "A babel plugin to add Build time theme resolution for Nativebase",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cd example && rm -rf .expo && yarn start",
    "dev:web": "cd example && rm -rf .expo && yarn web",
    "dev:android": "cd example && rm -rf .expo && yarn android",
    "dev:ios": "cd example && rm -rf .expo && yarn ios",
    "build": "babel index.js -d lib",
    "publish": "yarn build && yarn version && npm publish"
  },
  "author": "Rohit Singh",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.6"
  },
  "peerDependencies": {
    "native-base": "*"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "native-base": "3.5.0-alpha.5"
  },
  "engines": {
    "node": ">=10.24.1"
  }
}
